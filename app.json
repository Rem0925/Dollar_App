{
  "expo": {
    "name": "VeGreen",
    "slug": "vegreen",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/SplashScreen.png",
      "resizeMode": "contain",
      "backgroundColor": "#121212"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.REM_25.vegreen",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#121212"
      },
      "edgeToEdgeEnabled": false
    },
    "plugins": [
      [
        "react-native-android-widget",
        {
          "widgets": [
            {
              "name": "VeGreenWidget",
              "label": "VeGreen",
              "description": "Tasas del d√≠a",
              "previewImage": "./assets/Widget-Icon.png",
              "resizeMode": "horizontal|vertical",
              "updatePeriodMillis": 1800000,
              "minWidth": "150dp",
              "minHeight": "110dp"
            }
          ]
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "99fa41d4-4976-4314-a021-87a72980b100"
      }
    },
    "owner": "rem_25"
  }
