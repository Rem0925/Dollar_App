{
  "expo": {
    "name": "VeGreen",
    "slug": "vegreen",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/SplashScreen.png",
      "resizeMode": "contain",
      "backgroundColor": "#121212"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.REM_25.vegreen",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#121212"
      },
      "edgeToEdgeEnabled": false
    },
    "extra": {
      "eas": {
        "projectId": "45e8dcea-412f-4927-949a-f464505783e1"
      }
    },
    "plugins": [
      [
        "react-native-android-widget",
        {
          "widgets": [
            {
              "name": "VeGreenWidget",
              "label": "VeGreen",
              "description": "Tasas del d√≠a",
              "previewImage": "./assets/icon.png",
              "resizeMode": "horizontal|vertical",
              "updatePeriodMillis": 1800000,
              "minWidth": "60dp", 
              "minHeight": "60dp"
            }
          ]
        }
      ]
    ]
  }
}
